<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iotp">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".ListActivity"></activity>
        <activity android:name=".AlarmActivity" />
        <activity android:name=".MemberInitActivity" />
        <activity android:name=".MemberActivity" />
        <activity android:name=".logLookupActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".ResultActivity" />
        <activity android:name=".selectdocumentActivity" />
        <activity android:name=".memberInsertActivity"/>
        <activity android:name=".userInfoActivity"/>
        <activity android:name=".renewalDateActivity"/>
        <activity android:name=".documentActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".Broadcast"></receiver>
    </application>

</manifest>